      <!-- sing up -->
      <div class="container head">
          <div class="row justify-content-center  mt-5">
              <div class="col-xl-6 header">
                  <h1 class="text-center header">Sign Up <br> As a Parent</h1>
                <form  action="/suParent" method="post">
                    <div class="mb-3">
                      <label for="exampleInputName" class="form-label">Full Name</label>
                      <input type="text" class="form-control" id="exampleInputName" name="name" placeholder="Enter Your Full Name" required unique> 
                      
                    </div>
                    <div class="mb-3">
                      <label for="exampleInputEmail1" class="form-label">Email address</label>
                      <input type="email" class="form-control" id="exampleInputEmail1" name="email" placeholder="Enter Your Valid Email" required>
                    </div>
                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Password</label>
                        <input type="password" class="form-control" id="exampleInputPassword1" name="password" placeholder="Enter Your Password" required>
                      </div>
                    <div class="mb-3">
                        <label for="exampleInputPassword1" class="form-label">Confirm-Password</label>
                        <input type="password" class="form-control" id="exampleInputPassword1" name="RePassword" placeholder="Retype Your Password" required>
                      </div>
                      
                      <div class="mb-3">
                        <label for="exampleInputNid" class="form-label">NID Number</label>
                        <input type="Number" class="form-control" id="exampleInputNidr" name="nid" placeholder="Enter Your Nid Number" required 
                      </div>
                    <div class="mb-3">
                        <label for="exampleInputNumber" class="form-label">Contact Number</label>
                        <input type="Number" class="form-control" id="exampleInputNumber" name="number" placeholder="Enter Your Contact Number" required
                      </div>
                    <div class="mb-3">
                        <label for="exampleInputAdress" class="form-label">Present Address</label>
                        <input type="text" class="form-control" id="exampleInputAdress" name="address" placeholder="Enter Your Present Address"required
                      </div>
                    
                      <input type="submit" value="Register" class="btn btn-primary mt-4">
                     
                      <br>
                      
                  </form>
                
              </div>
          </div>
      </div>
    

    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    
